{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "outDir": "dist",
        "noEmit": false,
        "declaration": false,
        "sourceMap": true,
        "target": "ES2023",
        "module": "Node16",
        "moduleResolution": "Node16"
    },
    "include": [
        "src/deepeye/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist",
        "src/deepeye/**/*.test.ts"
    ]
}
